  "servers": [
    {
      "url": "http://localhost:3001/",
      "description": "Main api"
    }
  ],
  "paths": {
    "/login": {
      "post": {
        "summary": "User login",
        "tags": ["User"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Login"
              },
              "examples": {
                "bola8": {
                  "value": {
                    "email": "bola8@gmail.com",
                    "password": "vaiRolarLilFarm?"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/register": {
      "post": {
        "summary": "User register",
        "tags": ["User"],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Register"
              },
              "examples": {
                "bola8": {
                  "value": {
                    "username": "bola8",
                    "email": "bola8@gmail.com",
                    "password": "vaiRolarLilFarm?"
                  }
                },
                "irene": {
                  "value": {
                    "username": "irene",
                    "email": "irene@gmail.com",
                    "password": "umaSenhoraJovem"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "409": {
            "description": "User already exists"
          },
          "201": {
            "description": "User created successfully"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Register": {
        "properties": {
          "username": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "Login": {
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      }
    }
  }
}
